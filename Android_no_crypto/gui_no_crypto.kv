<MainWindow>:
    name: "main"
	background_color: (82, 82, 122)
    FloatLayout:
    	Image:
			background_color: (1, 1, 1, 1)
    		source:'white_logo_transparent.png'
			allow_stretch: True
    		size_hint:0.80, 0.40
    		pos_hint:{"x":0.1, "top":1.00}
		Label:
			text:"Welcome"
			size_hint:0.5, 0.05
			pos_hint:{"x":.25, "top":0.58}
			halign: 'center'
			font_size:60
		Label:
		    text:"to"
			size_hint:0.5, 0.05
			pos_hint:{"x":.25, "top":0.53}
			halign: 'center'
			font_size:60
		Label:
		    text:"Next Capital Tech"
			size_hint:0.5, 0.05
			pos_hint:{"x":.26, "top":0.48}
			halign: 'center'
			font_size:60
		Button:
			text: "Sign In"
			size_hint:0.50, 0.07
			pos_hint:{"x":0.25, "top":.42}
			on_release: app.root.current = 'second'
		Button:
			text: "Start/Stop Listening"
			size_hint:0.50, 0.07
			pos_hint:{"x":0.25, "top":.34}
			on_release: root.start_listening()
		Button:
			text: "Payment Options"
			size_hint:0.50, 0.07
			pos_hint:{"x":0.25, "top":.26}
			on_release: app.root.current = 'paymentOptions'
		Button:
			text: "Edit Credentials"
			size_hint: .50, .07
			pos_hint: {"x":.25, 'top':.18}
			on_release: app.root.current = 'credsCrypto'
		Button:
			text: "Donation"
			size_hint:0.50, 0.07
			pos_hint:{"x":0.25, "top":.10}
			on_release: app.root.current = 'donation'

<PaymentOptions>:
	name: 'paymentOptions'
	FloatLayout:
		Label:
			text: 'Select Payment Option'
			size_hint: .5, .1
			pos_hint: {'x':.25, 'top':.85}
			font_size: 60
		Label:
			text: 'Enter Discount Code: '
			size_hint: .3, .05
			pos_hint: {'x':.2, 'top':.75}
			font_size: 30
		TextInput:
			id: discountCode
			multiline: False
			size_hint: 0.3, 0.05
			pos_hint:{"x":0.50, "top":0.75}
			write_tab: False
		Label:
			id: discountCodeValidity
			text: ''
			size_hint: .2, .05
			pos_hint: {'x':.8, 'top':.75}
			font_size: 30
		Button:
			text: 'Check discount code'
			size_hint: .5, .07
			pos_hint: {'x':.25, 'top':.65}
			on_release: root.check_discount()
		Button:
			text: 'Pay by card'
			size_hint: .5, .07
			pos_hint:{'x':.25, 'top':.55}
			on_release: root.pay()
		Button:
			text: 'Refresh Payment Status'
			size_hint: .5, .07
			pos_hint:{'x':.25, 'top':.45}
			on_release: root.check()
		Button:
			text: 'Terms of Service'
			size_hint: .5, .07
			pos_hint: {'x':.25, 'top':.30}
			on_release: app.root.current = 'tos'
		Label:
			text: ''
			size_hint: .5, .05
			pos_hint:{'x':.25, 'top':.15}
			id: paymentStatus
		Button:
			text: 'Cancel'
			size_hint: .5, .07
			pos_hint:{'x':0, 'top':0.07}
			on_release: app.root.current = 'main'

